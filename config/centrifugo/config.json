{
   "client": {
      "allow_anonymous_connect_without_token": true,
      "allowed_origins": [
         "*"
      ]
   },
   "http_api": {
      "key": "my_api_key"
   },
   "admin": {
      "password": "password",
      "secret": "secret",
      "enabled": true
   },
   "channel": {
      "without_namespace": {
         "allow_subscribe_for_client": true,
         "allow_subscribe_for_anonymous": true,
         "presence": true,
         "history_size": 10,
         "history_ttl": "30s"
      },
      "namespaces": [
         {
            "name": "news",
            "allow_subscribe_for_client": true,
            "allow_history_for_client": true,
            "allow_subscribe_for_anonymous": true,
            "allow_history_for_anonymous": true,
            "force_recovery": true,
            "history_size": 100,
            "history_ttl": "300s"
         }
      ]
   }
}